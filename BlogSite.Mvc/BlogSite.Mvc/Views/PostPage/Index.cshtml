@model ListAndOnePostDto;

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
    
    @await Html.PartialAsync("HeadPartialForPost",Model)
<body>
    <!-- Navigation-->
    @await Component.InvokeAsync("_NavBarPartial")
    <!-- Page Header-->
    @await Component.InvokeAsync("_CoverPartial", Model)
    <!-- post Content-->
    @await Html.PartialAsync("Content", Model)
    <!-- Footer-->
    @await Component.InvokeAsync("_FooterPartial")
    <!-- Js -->
    @await Component.InvokeAsync("_ScriptsPartial")
    </body>
</html>
