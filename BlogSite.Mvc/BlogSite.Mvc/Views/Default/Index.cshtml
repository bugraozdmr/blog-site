@model FromRouteDto

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
    
    @await Component.InvokeAsync("_HeadPartial")

<body>
        <!-- Navigation-->
        @await Component.InvokeAsync("_NavBarPartial")
        <!-- Page Header-->
        @await Component.InvokeAsync("_PageHeaderPartial")
        <!-- Main Content-->
        @await Component.InvokeAsync("_MainContentPartial",Model)
        <!-- Footer-->
        @await Component.InvokeAsync("_FooterPartial")
        <!-- Js -->
        @await Component.InvokeAsync("_ScriptsPartial")
    </body>
</html>
